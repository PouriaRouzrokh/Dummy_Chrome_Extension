{
  "manifest_version": 3,
  "name": "API Request Handler",
  "version": "1.0",
  "description": "Send custom API requests using CURL schema",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost/*",
    "https://*/*",
    "http://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "popup.html"
  }
}